{
  "expo": {
    "name": "vvinamp",
    "slug": "vvinamp_expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vvinamp.png",
    "scheme": "vvinampexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crisandolindev.vvinamp-expo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vvinamp.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.crisandolindev.vvinamp_expo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vvinamp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0355f686-4cc5-4204-a18b-444157f28e5c"
      }
    }
  }
}
